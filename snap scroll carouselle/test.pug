- const items = ['Item 1', 'Item 2', 'Item 3  ', 'Item 4', 'Item 5', 'Item 6', 'Item 7', 'Item 8', 'Item 9', 'Item 10'];

.list-wrap
  ul.list 
    each item in items 
      li.list__item
        .list__item__content #{ item }
  a.button.previous(type="button") &lt;
  a.button.next(type="button") &gt;
